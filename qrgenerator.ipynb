   "source": [
    "import qrcode\n",
    "\n",
    "url = input(\"Enter the URL: \").strip()\n",
    "filepath = \"D:\\\\qrcodeimg\\\\qrcodeimg.png\"\n",
    "\n",
    "qr = qrcode.QRCode()\n",
    "qr.add_data(url)\n",
    "\n",
    "img = qr.make_image()\n",
    "img.save(filepath)\n",
    "\n",
    "print(\"QR Code generated\")"
   ]
